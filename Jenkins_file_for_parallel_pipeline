pipeline {
  agent none
  stages {
     stage('Non-Parallel Stage') {
          agent {
             label "master"
          }
          steps {
             echo "This stage will be executed first"
          }
     }

     stage('Run Tests') {
        parallel {
           stage('Test on Windows') {
                 agent {
                    label "ndee" 
                 }
                 steps {
                    echo "Task1 on Agent";
                 }
           }
           stage('Test on master') {
                 agent {
                    label "master" 
                 }
                 steps {
                    echo "Task 1 on master";
                 }
           }
        }
     }
